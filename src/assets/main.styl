@import url('https://fonts.googleapis.com/css2?family=Inter&display=swap');

body {
  font-family: 'Inter', sans-serif;
}
.h-100vh {
  height: 100vh;
}
*, ul, li, ol {
  box-sizing border-box
  margin: 0;
  padding: 0;
  list-style: none;
}
h1, .h1 {
  font-size: 20px;
  font-style: normal;
  font-weight: 700;
  line-height: 28px;
}
